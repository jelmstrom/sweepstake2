web: java -Ddw.server.rootPath="/rest/*" -Ddw.server.applicationConnectors[0].port=$PORT -Ddw.neoConfiguration.credentials=$NEO_CRED -Ddw.neoConfiguration.user=$NEO_USER -Ddw.neoConfiguration.location=$NEO_URL -Ddw.neoConfiguration.purge=false  -jar build/target/vmtips-gradle/dist/vmtips-gradle-2.0.0-SNAPSHOT.jar server

